{
    "name": "opensoft/inkrouter-php-sdk",
    "description": "Small sdk for connecting to OpenSoft Job Submission Interface",
    "authors": [
        {
            "name": "Opensoft",
            "email": "info@opensoftdev.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Opensoft\\InkRouterSdk\\": "src/InkRouter/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "require": {
        "php": ">=7.2.0",
        "ext-curl": ">=7.2",
        "ext-dom": "20031129",
        "ext-soap": ">=7.2.0",
        "ext-xmlwriter": ">=7.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "8.*"
    }
}
